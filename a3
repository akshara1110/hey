import numpy as np
data_type=[('name','S15'),('class',int),('height',float)]
student_details=[('James',5,48.5),('Nail',6,52.5),('paul',5,42.10),('pit',5,40.11)]
student=np.array(student_details,dtype=data_type)
print("original array:")
print(student)
print("sort by class,then height if class are equal:")
print(np.sort(student,order=['class','height']))
